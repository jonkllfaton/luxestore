{
  "testEnvironment": "node",
  "setupFilesAfterFramework": [],
  "globalSetup": null,
  "setupFiles": ["./tests/setup.js"],
  "collectCoverageFrom": [
    "routes/**/*.js",
    "models/**/*.js",
    "middleware/**/*.js",
    "controllers/**/*.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "testMatch": ["**/tests/**/*.test.js"],
  "verbose": true
}
